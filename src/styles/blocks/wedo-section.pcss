
.wedo-section {
    height: auto;
    width: 100%;
    background: #f2f2f2;
}

.wedo-container {
    display: flex;
    justify-content: center;
    align-items: center;
}

.wedo-title {
    display: flex;
    font-size: 28px;
    text-transform: uppercase;
    margin-bottom: 3%;
    margin-top: 10%;
}

.wedo-title-grey {
    color: $grey-text;
    margin-right: 5px;
}

.wedo-title-red {
    color: $red;
}